<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
	
	
    <servlet>
    	<servlet-name>FundTransfer</servlet-name>
    	<servlet-class>com.customer.FundInsert</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>FundTransfer</servlet-name>
    	<url-pattern>/insert</url-pattern>
    </servlet-mapping>

	
	
	<servlet>
		<servlet-name>ViewTransfer</servlet-name>
		<servlet-class>com.customer.ViewTransfer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewTransfer</servlet-name>
		<url-pattern>/select</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>Delete</servlet-name>
		<servlet-class>com.customer.Deletetransfe</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Delete</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet>
		<servlet-name>update</servlet-name>
		<servlet-class>com.customer.Update</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>update</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	
	
	
	
	

</web-app>